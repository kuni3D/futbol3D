 {
     "$schema": "node_modules/wrangler/config-schema.json",
     "name": "multiplayer-game",
     "main": "src/index.js", // Cambia a "index.js" si no usas una carpeta src/
     "compatibility_date": "2025-03-21",
     "migrations": [
         {
             "new_classes": ["GameRoom"], // Cambié "new_sqlite_classes" a "new_classes" y "MyDurableObject" a "GameRoom"
             "tag": "v1"
         }
     ],
     "durable_objects": {
         "bindings": [
             {
                 "class_name": "GameRoom", // Cambié "MyDurableObject" a "GameRoom"
                 "name": "GAME_ROOM"       // Mantengo el nombre del binding como "GAME_ROOM"
             }
         ]
     },
     "observability": {
         "enabled": true
     }
 }
